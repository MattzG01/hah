	left: -20px;
	min-width: 700px;
	min-height: 275px;
	border-radius: 0px 90px 0px 90px;
	border-style: solid;
	border-color:#333;
	background-color: #333;
}
div.pad{
	margin-top: 40px;
	margin-left: 20px;
	margin-right: 20px;
	min-width: 815px;
	border-radius: 150px 0px 0px 150px;
	border-style: solid;
	border-color:#555;
	background-color: #555;
}
div.padinvert{
	margin-top: 40px;
