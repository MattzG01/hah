	padding-left: 30px;
	padding-right: 20px;
}
th{
	background-color: #454545;
}
td{
	padding-left: 20px;
	border-bottom: 1px solid #777;
}
tr:nth-child(even){
	background-color: #454545;
}
th.generalbubble{
	border-radius: 60px 60px 0px 0px;
	border-style: solid;
	border-color:#333;
	background-color: #333;
}
td:hover{
	padding-left: 20px;
	border-bottom: 1px solid #777;
	border-right: 3px solid #777;
	background-color: #777;
}
